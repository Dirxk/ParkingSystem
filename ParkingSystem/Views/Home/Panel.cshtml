@{
    ViewData["Title"] = "Panel";
}


<link href="https://cdn.grapecity.com/wijmo/5.latest/styles/wijmo.min.css" rel="stylesheet">
<script src="https://code.jquery.com/jquery-3.6.0.min.js"></script>
<link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.5.1/css/all.min.css" rel="stylesheet">

<div class="container mt-4">
    <div class="row">
        <div class="col-md-6 mb-4">
            <div class="card">
                <div class="card-body">
                    <h5 class="card-title">Ganancias de la Semana</h5>
                    <p class="card-text">Visualización de las ganancias de esta semana.</p>
                    <div id="chartGananciasSemana" style="height: 300px;"></div>
                </div>
            </div>
        </div>
        <div class="col-md-6 mb-4">
            <div class="card">
                <div class="card-body">
                    <h5 class="card-title">Ganancias del Mes</h5>
                    <p class="card-text">Visualización de las ganancias mensuales.</p>
                    <div id="chartGananciasMes" style="height: 300px;"></div>
                </div>
            </div>
        </div>
        <div class="col-md-12">
            <div class="card">
                <div class="card-body">
                    <h5 class="card-title">Total de Vehículos</h5>
                    <p class="card-text">Distribución total de vehículos.</p>
                    <div id="chartTotalVehiculos" style="height: 300px;"></div>
                </div>
            </div>
        </div>
    </div>
</div>
